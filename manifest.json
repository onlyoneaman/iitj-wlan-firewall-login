{
  "short_name": "Wlan Login",
  "name": "Logins into Wlan",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "extension/build/index.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png" 
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://gateway.iitj.ac.in:1003/login?*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'sha256-fdAfMKZtCTRPKMRznGwonJHC++vCCrQS7XFnVrzSXAE=' 'sha256-YD8w9VLrdvqI5/tFVmpT+1337ly+e/JvPrkRxYZo/w4=' 'sha256-xvDP1jsKqlCFyWpplHqwBXEq8jkfyuOiAXTErLeOGgc='; object-src 'self';"
}
